    margin-bottom: 0;
}

#exposed-property-scroll .unity-scroller__low-button {
    display: none;
}

#exposed-property-scroll .unity-scroller__high-button {
    display: none;
}

#exposed-property-scroll .unity-base-slider__tracker {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

#exposed-property-scroll .unity-base-slider__dragger {
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-right-width: 1px;
    border-left-color: rgb(34, 34, 34);
    border-right-color: rgb(34, 34, 34);
    border-top-color: rgb(34, 34, 34);
    border-bottom-color: rgb(34, 34, 34);
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    border-left-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgb(77, 77, 77);
}

#exposed-property-scroll .unity-base-slider__dragger:hover {
    background-color: rgb(196, 196, 196);
}

#exposed-property-container {
}

.unity-base-field {
    margin-left: 0;
    margin-right: 0;
}
